import math
import cmath

b = 0.0
c = 1e50

D = b**2- 4*c 

def squadrix_viet (b, c):
  
  
  if D >= 0:
    sqD = math.sqrt (D)
    
    
    if b >= 0: #определям подходящий знак b
     
      if c == 0 :
        
        x1 = 0.0
        x2 = -1.0 * b
      
      else :
      
        x1 = (-b - sqD)/2.0
        x2 = c/x1
    
    else:
      
      x1 = (-b + sqD)/2.0
      x2 = c/x1
    
  
  else:
    sqD = cmath.sqrt (D)
    
    x1 = (-b + sqD)/2.0
    x2 =  c/x1
    
  
  return x1, x2


